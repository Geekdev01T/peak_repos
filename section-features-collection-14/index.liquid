{% assign price_size_mobile = section.settings.price_size_mobile %}
{% assign price_regular_color = section.settings.price_regular_color %}
{% assign price_compare_color = section.settings.price_compare_color %}

{% assign image_radius = section.settings.image_radius %}
{% assign image_border_w = section.settings.image_border_w %}
{% assign image_border_color = section.settings.image_border_color %}

{% assign btn_size = section.settings.btn_size %}
{% assign btn_size_mobile = section.settings.btn_size_mobile %}
{% assign btn_weight = section.settings.btn_weight %}
{% assign btn_icon_w = section.settings.btn_icon_w %}
{% assign btn_icon_h = section.settings.btn_icon_h %}
{% assign btn_radius = section.settings.btn_radius %}
{% assign btn_border_w = section.settings.btn_border_w %}
{% assign btn_color = section.settings.btn_color %}
{% assign btn_color_hover = section.settings.btn_color_hover %}
{% assign btn_border_color = section.settings.btn_border_color %}
{% assign btn_border_color_hover = section.settings.btn_border_color_hover %}
{% assign btn_bg = section.settings.btn_bg %}
{% assign btn_bg_hover = section.settings.btn_bg_hover %}

{% assign badge_size = section.settings.badge_size %}
{% assign badge_size_mobile = section.settings.badge_size_mobile %}
{% assign badge_radius = section.settings.badge_radius %}
{% assign badge_border_w = section.settings.badge_border_w %}
{% assign badge_color = section.settings.badge_color %}
{% assign badge_border_color = section.settings.badge_border_color %}
{% assign badge_bg = section.settings.badge_bg %}

{% assign slide_radius = section.settings.slide_radius %}
{% assign slide_border_w = section.settings.slide_border_w %}
{% assign slide_border_color = section.settings.slide_border_color %}
{% assign slide_bg = section.settings.slide_bg %}

{% assign dots_radius = section.settings.dots_radius %}
{% assign dots_border_w = section.settings.dots_border_w %}
{% assign dots_border_color = section.settings.dots_border_color %}
{% assign dots_bg = section.settings.dots_bg %}

{% assign nav_btn_w = section.settings.nav_btn_w %}
{% assign nav_btn_h = section.settings.nav_btn_h %}
{% assign nav_btn_radius = section.settings.nav_btn_radius %}
{% assign nav_btn_border_w = section.settings.nav_btn_border_w %}
{% assign nav_btn_color = section.settings.nav_btn_color %}
