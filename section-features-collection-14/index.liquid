{% assign nav_btn_border_color = section.settings.nav_btn_border_color %}
{% assign nav_btn_border_color_hover = section.settings.nav_btn_border_color_hover %}
{% assign nav_btn_bg = section.settings.nav_btn_bg %}

{% assign full_width = section.settings.full_width %}
{% assign section_mt = section.settings.section_mt %}
{% assign section_mb = section.settings.section_mb %}
{% assign full_border = section.settings.full_border %}
{% assign section_radius = section.settings.section_radius %}
{% assign section_border_w = section.settings.section_border_w %}
{% assign section_border_color = section.settings.section_border_color %}
{% assign section_bg = section.settings.section_bg %}

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css">
<style>
  .section-{{section.id}} {
      max-width: {% if full_width %} 100% {% else %} 1200px {% endif %};
      {% if full_border %}
        border: {{section_border_w}}px solid {{section_border_color}};
      {% endif %}
      background-color: {{section_bg}};
      border-radius: {{section_radius}}px;
      margin: 0 auto;
      margin-top: {{section_mt}}px !important;
      margin-bottom: {{section_mb}}px !important;
      overflow: hidden;
  }

  .section-{{section.id}}-settings {
      margin: 0 auto;
      padding-top: 27px;
      padding-bottom: 27px;
      padding-left: 1.5rem;
      padding-right: 1.5rem;
  }

  .featured-heading-{{section.id}} {
      text-align: center;
      margin-top: 8px;
  }

  .featured-heading-{{section.id}} * {
      margin: 0;
      font-size: {{ heading_size_mobile }}px;
      color: {{ heading_color }};
      line-height: 130%;
      text-transform: unset;
      text-decoration: none;
      word-break: break-word;
      font-weight: {{ heading_weight }};
  }

  .featured-sub-heading-{{section.id}} {
      text-align: center;
  }

  .featured-sub-heading-{{section.id}} * {
      margin: 0;
      font-size: {{sub_heading_size_mobile}}px;
      color: {{sub_heading_color}};
      line-height: 130%;
      text-transform: unset;
      text-decoration: none;
      word-break: break-word;
      font-weight: {{sub_heading_weight}};
  }

  .featured-slider-{{section.id}} {
      position: relative !important;
      margin-top: 0px !important;

      padding-top: calc(30px + 14px) !important;

      margin-left: -1.5rem !important;
      margin-right: -1.5rem !important;
      padding-left: 1.5rem !important;
      padding-right: 1.5rem !important;
      padding-bottom: 50px !important;
  }

  .featured-slide-{{section.id}} {
      overflow: visible !important;
      position: relative !important;
      display: flex !important;
      flex-direction: column !important;
      border: {{slide_border_w}}px solid {{slide_border_color}};
      border-radius: {{slide_radius}}px;
      background: {{slide_bg}};
  }


  .featured-product-image-{{section.id}} {
      position: relative;
      height: 100%;
  }

  .featured-product-image-{{section.id}} img,
  .featured-product-image-{{section.id}} svg {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
      border: {{image_border_w}}px solid {{image_border_color}};
      border-radius: {{image_radius}}px;
  }

  .featured-product-image-{{section.id}} svg {
      background-color: #AFAFAF;
  }

  .featured-badge-{{section.id}} {
      position: absolute;
      top: 0;
      left: 50%;
      transform: translate(-50%, -50%);
      z-index: 2;
      display: flex;
      align-items: center;
      justify-content: center;
      padding: 4px 8px;
      background-color: {{badge_bg}};
      border: {{badge_border_w}}px solid {{badge_border_color}};
      border-radius: {{badge_radius}}px;
      margin: 0;
      font-size: {{badge_size_mobile}}px;
      color: {{badge_color}};
      word-break: break-word;
      line-height: 130%;
      overflow: hidden;
      text-align: center;
      text-transform: unset;
      text-decoration: none;
      opacity: 0;
      transition: all 0.25s ease 0s;
  }

  .featured-slide-{{section.id}}.swiper-slide-active .featured-badge-{{section.id}},
  .featured-slide-{{section.id}}.swiper-slide-active .featured-product-title-{{section.id}},
  .featured-slide-{{section.id}}.swiper-slide-active .featured-product-price-{{section.id}} {
      opacity: 1;
      transition: all 0.25s ease 0s;
  }

  .featured-button-{{section.id}} {
      display: flex;
      align-items: center;
      justify-content: center;
      gap: 10px;
      width: 100%;
      max-width: fit-content;
      margin: 0;
      font-size: {{btn_size_mobile}}px;
      color: {{btn_color}};
      line-height: 100%;
      text-align: center;
      text-transform: unset;
      text-decoration: none;
      padding: 14px 32px;
      border-radius: {{btn_radius}}px;
      transition: all 0.25s ease 0s;
      background-color: transparent;
      border: 0px;
      cursor: pointer;
      font-weight: {{btn_weight}};
  }

  .featured-button-mobile-{{section.id}} {
      position: relative;
      top: unset;
      left: unset;
      transform: none;
      display: block;
      margin-top: 16px;
  }

  .featured-button-desktop-{{section.id}} {
      position: absolute;
      top: 95%;
      left: 50%;
      transform: translateX(-50%);
      display: none;
  }

  .featured-button-inner-{{section.id}} {
      margin: 0px;
      display: flex;
      align-items: center;
      justify-content: center;
      gap: 10px;
  }

  .featured-button-{{section.id}}:hover {
      color: {{btn_color_hover}};
      transition: all 0.25s ease 0s;
  }

  .featured-button-{{section.id}} svg {
      width: {{btn_icon_w}}px;
      height: {{btn_icon_h}}px;
      border: none;
      background-color: transparent;
  }

  .featured-button-{{section.id}} svg path {
      fill: {{btn_color}};
      transition: all 0.25s ease 0s;
  }

  .featured-button-{{section.id}}:hover svg path {
      fill: {{btn_color_hover}};
      transition: all 0.25s ease 0s;
  }

  .feature-product-content-{{section.id}} {
      display: flex;
      flex-direction: column;
      align-items: center;
  }

  .featured-product-title-{{section.id}} {
      margin: 0;
      opacity: 0;
      width: 100%;
      text-align: center;
      margin-top: 20px;
      font-size: {{prod_name_size_mobile}}px;
      color: {{prod_name_color}};
      line-height: 150%;
      text-transform: unset;
      word-break: break-word;
      transition: all 0.25s ease 0s;
      font-weight: {{prod_name_weight}};
  }

  .featured-product-price-{{section.id}} {
      opacity: 0;
      margin-top: 6px;
      width: 100%;
      display: flex;
      align-items: center;
      gap: 5px;
      justify-content: center;
  }

  .featured-product-price-{{section.id}} * {
      margin: 0;
      text-align: center;
      font-size: {{price_size_mobile}}px;
      color: {{price_regular_color}};
      line-height: 130%;
      text-transform: unset;
      word-break: break-word;
      transition: all 0.25s ease 0s;
  }

  .featured-product-compare-price-{{section.id}} {
      text-decoration: line-through;
      color: {{price_compare_color}};
  }

  .featured-dots-body-{{section.id}} {
      display: none;
      transition: all 0.5s ease 0s;
      pointer-events: none;
      opacity: 1;
  }

  .featured-dots-{{section.id}} {
      position: absolute;
      width: 8%;
      top: 38%;
      height: 45px;
  }

  .featured-dots-{{section.id}}:first-child {
      left: 23%;
  }

  .featured-dots-{{section.id}}:last-child {
      right: 23%;
  }

  .featured-dot-{{section.id}} {
      position: absolute;
      display: block !important;
      border-radius: {{dots_radius}}%;
      border: {{dots_border_w}}px solid {{dots_border_color}};
      background-color: {{dots_bg}};
  }

  .featured-dots-{{section.id}}:first-child .featured-dot-{{section.id}}:nth-child(1) {
      left: 0;
      bottom: 0;
  }

  .featured-dots-{{section.id}}:first-child .featured-dot-{{section.id}}:nth-child(2) {
      left: 50%;
      top: 50%;
      transform: translate(-50%, -50%)
  }

  .featured-dots-{{section.id}}:first-child .featured-dot-{{section.id}}:nth-child(3) {
      right: 0;
      top: 0;
  }


  .featured-dots-{{section.id}}:last-child .featured-dot-{{section.id}}:nth-child(1) {
      left: 0;
      top: 0;
  }

  .featured-dots-{{section.id}}:last-child .featured-dot-{{section.id}}:nth-child(2) {
      left: 50%;
      top: 50%;
      transform: translate(-50%, -50%)
  }

  .featured-dots-{{section.id}}:last-child .featured-dot-{{section.id}}:nth-child(3) {
      right: 0;
      bottom: 0;
  }

  .featured-buttons-{{section.id}} {
      width: 100%;
      position: relative;
      display: flex;
      align-items: center;
      gap: 16px;
      margin-top: 20px;
      justify-content: center;
  }

  .featured-btn-prev-{{section.id}},
  .featured-btn-next-{{section.id}} {
      display: flex;
      width: {{nav_btn_w}}px;
      height: {{nav_btn_h}}px;
      border: 0px;
      border: {{nav_btn_border_w}}px solid {{nav_btn_border_color}};
      border-radius: {{nav_btn_radius}}px;
      background: {{nav_btn_bg}};
      align-items: center;
      justify-content: center;
      transition: all 0.3s ease 0s;
      cursor: pointer;
      -webkit-appearance: none;
      /* box-shadow: 0px 0px 10px 0px rgba(0,0,0,.15); */
  }

  .featured-btn-prev-{{section.id}} .animate-icon,
  .featured-btn-next-{{section.id}} .animate-icon {
      pointer-events: none;
      place-items: center;
      display: grid;
  }

  .featured-btn-prev-{{section.id}} .animate-icon:before,
  .featured-btn-next-{{section.id}} .animate-icon:before {
      content: "";
      grid-area: 1 / -1;
      transition: all .2s ease;
      width: 9px;
      height: 9px;
      transform: rotate(calc(1 * 45deg));
      box-sizing: border-box;
      border-style: solid;
      border: 0px;
      border-right: 2px solid currentColor;
      border-top: 2px solid currentColor;
      position: relative;
      left: -1px;
  }

  .featured-btn-prev-{{section.id}} .animate-icon:before {
      left: unset;
      right: -1px;
      border-right: 0px;
      border-top: 0px;
      border-bottom: 2px solid currentColor;
      border-left: 2px solid currentColor;
  }

  .featured-btn-prev-{{section.id}} .animate-icon:after,
  .featured-btn-next-{{section.id}} .animate-icon:after {
      content: "";
      grid-area: 1 / -1;
      transition: all .2s ease;
      width: 8.7px;
      height: 1.6px;
      transform-origin: center;
      opacity: 0;
      background: currentColor;
      transform: scaleX(.5);
  }

  .featured-btn-prev-{{section.id}} span,
  .featured-btn-next-{{section.id}} span {
      color: {{nav_btn_color}};
  }

  .featured-btn-prev-{{section.id}}.swiper-button-disabled,
  .featured-btn-next-{{section.id}}.swiper-button-disabled {
      opacity: 0.5;
  }

  .featured-sub-heading-{{section.id}} * {
      font-family: Montserrat, sans-serif;
      font-weight: {{sub_heading_weight}};
      font-style: normal;
  }

     .featured-product-image-{{section.id}} {
      aspect-ratio: 12/12;
  }


  .featured-button-{{section.id}} {
      background-color: {{btn_bg}};
      border: {{btn_border_w}}px solid {{btn_border_color}};
  }

  .featured-button-{{section.id}}:hover {
      background-color: {{btn_bg_hover}};
      border: {{btn_border_w}}px solid {{btn_border_color_hover}};
  }


  .featured-button-{{section.id}} {
      margin-left: auto;
      margin-right: auto;
  }

  .featured-btn-prev-{{section.id}}:hover,
  .featured-btn-next-{{section.id}}:hover {
      transition: all 0.3s ease 0s;
      border: {{nav_btn_border_w}}px solid {{nav_btn_border_color_hover}};
  }

  .featured-btn-prev-{{section.id}}:hover .animate-icon,
  .featured-btn-next-{{section.id}}:hover .animate-icon {
      color: {{nav_btn_color_hover}};
  }

   .featured-buttons-{{section.id}} {
      display: flex;
  }


  @media(min-width: 1024px) {

      .section-{{section.id}}-settings {
          padding: 0 5rem;
          padding-top: 36px;
          padding-bottom: 36px;
          padding-left: 5rem;
          padding-right: 5rem;
      }

      .featured-sub-heading-{{section.id}} {
          text-align: center;
      }

      .featured-sub-heading-{{section.id}} * {
          font-size: {{sub_heading_size}}px;
      }

      .featured-heading-{{section.id}} {
          text-align: center;
          margin-top: 12px;
      }

      .featured-heading-{{section.id}} * {
          font-size: {{ heading_size }}px;
      }

      .featured-slider-{{section.id}} {
          margin-top: 24px !important;
          padding: 0 70px !important;

          padding-top: calc(30px + 16px) !important;

          margin-left: -5rem !important;
          margin-right: -5rem !important;
          padding-left: 5rem !important;
          padding-right: 5rem !important;
          padding-bottom: 200px !important;
      }

      .featured-badge-{{section.id}} {
          padding: 8px 8px;
          font-size: {{badge_size}}px;
      }

      .featured-button-{{section.id}} {
          opacity: 0;
          padding: 14px 32px;
          font-size: {{btn_size}}px;
          margin: 0px !important;
      }

      .featured-slide-{{section.id}}:hover .featured-button-{{section.id}} {
          opacity: 1;
          transition: all 0.25s ease 0s;
      }

      .featured-button-mobile-{{section.id}} {
          display: none;
      }

      .featured-button-desktop-{{section.id}} {
          display: block;
      }

      .feature-product-content-{{section.id}} {
          align-items: center;
      }

      .featured-product-title-{{section.id}} {
          text-align: center;
          margin-top: 36px;
          font-size: {{prod_name_size}}px;
      }

      .featured-product-price-{{section.id}} {
          margin-top: 8px;
          justify-content: center;
      }

      .featured-product-price-{{section.id}} * {
          text-align: center;
          font-size: {{price_size}}px;
      }

      .featured-dots-body-{{section.id}} {
          display: block;
      }

      .featured-dot-{{section.id}} {
          width: 20px;
          height: 20px;
      }

      .featured-buttons-{{section.id}} {
          width: 80%;
          position: absolute;
          bottom: 10%;
          left: 10%;
          justify-content: center;
      }

      .featured-btn-prev-{{section.id}},
      .featured-btn-next-{{section.id}} {
          width: {{nav_btn_w}}px;
          height: {{nav_btn_h}}px;
          transition: all 0.25s ease 0s;
      }

      .featured-btn-prev-{{section.id}} .animate-icon:before,
      .featured-btn-next-{{section.id}} .animate-icon:before {
          width: 11px;
          height: 11px;
      }

      .featured-btn-prev-{{section.id}} .animate-icon:after,
      .featured-btn-next-{{section.id}} .animate-icon:after {
          width: 10.7px;
      }

      .featured-product-image-{{section.id}} {
          aspect-ratio: 12/12;
      }

      .featured-buttons-{{section.id}} {
          display: flex;
      }
  }

  @media(min-width: 1920px) {
      .featured-slider-{{section.id}} {
          padding-bottom: 300px !important;
      }
  }
</style>

<div
  class="section-{{section.id}} featured-{{section.id}}"
  id="section-featured-{{section.id}}"
>
  <div class="section-{{section.id}}-settings">
    <div class="featured-sub-heading-{{section.id}}">
      <p>{{ sub_heading }}</p>
    </div>

    <div class="featured-heading-{{section.id}}">
      <h2>{{ heading }}</h2>
    </div>

    <div class="featured-body-{{section.id}}">
      <div class="featured-slider-{{section.id}} swiper">
        <div class="swiper-wrapper" id="swiper-wrapper-{{section.id}}">
          {% for product in collection.products %}
            <div class="featured-slide-{{section.id}} swiper-slide">
              <p class="featured-badge-{{section.id}}">
                {{ product.tags.first }}
              </p>
              <div class="featured-product-image-{{section.id}}">
                <img
                  src="{{ product.featured_image | img_url:'master'}}"
                  alt="{{ product.title }}"
                  loading="lazy"
                >

                <a
                  href="{{ product.url }}"
                  class="featured-button-{{section.id}} featured-button-desktop-{{section.id}}"
                >
                  <p class="featured-button-inner-{{section.id}}">
                    Shop now

